@@ -27,9 +27,15 @@ static void ST7920_SWSPI_SND_8BIT(uint8_t val)
   for( i=0; i<8; i++ )
   {
     WRITE(ST7920_CLK_PIN,0);
+    #if F_CPU == 20000000
+      __asm__("nop\n\t"); 
