+    sgtPlus.lock();
+  } 
+  else {
+    sgtPlus.unlock();
+  }
+  if (value==sgtSlider.min()) {
+    sgtMinus.lock();
+  } 
+  else {
+    sgtMinus.unlock();
+  }
+}
